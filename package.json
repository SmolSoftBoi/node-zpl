    "devDependencies": {
        "@types/lodash": "^4.14.157",
        "@types/node": "^14.0.23",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "eslint": "^7.4.0",
        "nodemon": "^2.0.4",
        "rimraf": "^3.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.6"
    }
}
