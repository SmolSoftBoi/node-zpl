{
    "name": "@epickris/zpl",
    "version": "0.2.1",
    "description": "Build, tweak and render individual labels.",
    "repository": {
        "type": "git",
        "url": "git://github.com/EpicKris/node-zpl.git"
    },
    "bugs": {
        "url": "https://github.com/EpicKris/node-zpl/issues"
    },
    "engines": {
        "node": ">=10.17.0"
    },
    "main": "dist/index.js",
    "bin": {
        "zpl": "bin/zpl"
    },
    "scripts": {
        "lint": "eslint src/**/*.{js,ts,json}",
